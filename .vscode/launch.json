{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "LSS: eval_model_iou (trainval)_nuScenes",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "args": [
        "eval_model_iou",
        "trainval",
        "--modelf=./model525000.pt",
        "--dataroot=/data/nuscenes/"
