      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONUNBUFFERED": "1",
        "HYDRA_FULL_ERROR": "1"
      }
    },
    {
      "name": "LSS: eval_model_iou (trainval)_SimBEV",
